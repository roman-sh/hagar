{
  "name": "hagar-delivery",
  "type": "module",
  "version": "1.0.0",
  "description": "AI-powered store inventory assistant",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "start": "npm run build && node dist/index.mjs",
    "dev": "sh -c 'redis-cli ping > /dev/null 2>&1 || (echo \"Starting Redis...\" && redis-server --daemonize yes); tsx app/main.ts'",
    "test": "vitest",
    "build": "bunchee --format esm",
    "dev:build": "bunchee --watch --format esm",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.777.0",
    "@azure/ai-form-recognizer": "^5.0.0",
    "@azure/identity": "^4.8.0",
    "@bull-board/api": "^6.7.10",
    "@bull-board/hono": "^6.7.10",
    "@hono/node-server": "^1.14.3",
    "@types/qrcode-terminal": "^0.12.2",
    "axios": "^1.10.0",
    "bee-queue": "^1.7.1",
    "bull": "^4.16.5",
    "dotenv": "^16.4.7",
    "hono": "^4.7.5",
    "ioredis": "^5.6.0",
    "mongodb": "^6.15.0",
    "openai": "^4.98.0",
    "pdf-lib": "^1.17.1",
    "pdf-to-img": "^4.5.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "puppeteer-core": "18.2.1",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.34.2",
    "whatsapp-web.js": "^1.27.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "bunchee": "^6.5.4",
    "rollup-plugin-string": "^3.0.0",
    "tsx": "latest",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
